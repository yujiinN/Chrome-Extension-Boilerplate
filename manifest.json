{
  "manifest_version": 3,
  "name": "Facebook Sentiment Analyzer",
  "version": "1.1",
  "description": "Analyze sentiment of your latest Facebook posts using the Graph API.",
  "permissions": ["identity", "storage", "scripting"],
  "host_permissions": ["https://graph.facebook.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
